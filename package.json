{
  "name": "maestro-orchestration",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:workers": "tsc --project tsconfig.workers.json",
    "start": "next start",
    "lint": "next lint",
    "redis:start": "docker-compose up -d redis",
    "redis:stop": "docker-compose stop redis",
    "redis:logs": "docker-compose logs -f redis",
    "workers:build": "npm run build:workers",
    "workers:start": "bash scripts/start-workers.sh",
    "workers:logs": "pm2 logs",
    "workers:stop": "pm2 stop all",
    "workers:restart": "pm2 restart all",
    "agents:start": "bash scripts/start-agents.sh",
    "queues:stats": "curl http://localhost:3000/api/queues",
    "system:start": "npm run redis:start && npm run workers:build && npm run workers:start && npm run dev",
    "system:stop": "pm2 stop all && docker-compose stop"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.0",
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "@supabase/supabase-js": "^2.79.0",
    "bullmq": "^5.63.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.263.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
